<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
        "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
        "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <class name="com.serdarsenturk.TvShowsEntity" table="tv_shows" schema="Movies">
        <id name="id">
            <column name="id" sql-type="int unsigned"/>
        </id>
        <property name="seasons">
            <column name="seasons" sql-type="int" not-null="false"/>
        </property>
        <property name="episodes">
            <column name="episodes" sql-type="int" not-null="false"/>
        </property>
        <property name="episodesRuntime">
            <column name="episodesRuntime" sql-type="int" not-null="false"/>
        </property>
    </class>
</hibernate-mapping>